{
  "name": "your-score",
  "version": "1.0.1",
  "repository": "https://github.com/richardcyrus/music-catalog.git",
  "author": "Richard Cyrus <hello@rcyrus.com> (https://www.richardcyrus.com/)",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "axios": "1.2.5",
    "bcrypt": "5.1.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "debug": "4.3.4",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "helmet": "6.0.1",
    "jsonwebtoken": "9.0.0",
    "morgan": "1.10.0",
    "mysql2": "3.0.1",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "per-env": "1.0.2",
    "sequelize": "6.28.0"
  },
  "devDependencies": {
    "concurrently": "7.6.0",
    "cross-env": "7.0.3",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "husky": "^8.0.0",
    "json": "11.0.0",
    "lint-staged": "13.1.0",
    "micromatch": "4.0.5",
    "nodemon": "2.0.20",
    "prettier": "2.8.3",
    "sequelize-cli": "6.6.0"
  },
  "scripts": {
    "build": "cd client && yarn build",
    "client": "cd client && yarn start",
    "fix": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "install": "cd client && yarn install",
    "lint": "prettier --write '**/*.{js,json,scss}'",
    "migrate": "sequelize-cli db:migrate",
    "migrate:reset": "sequelize-cli db:migrate:undo:all && yarn run migrate",
    "prepare": "husky install",
    "start": "per-env",
    "start:development": "concurrently \"cross-env DEBUG=your-score:* nodemon ./bin/www\" \"yarn run client\"",
    "start:production": "node ./bin/www",
    "start:server": "DEBUG=your-score:* nodemon ./bin/www"
  },
  "engines": {
    "node": "18"
  },
  "volta": {
    "node": "18.13.0"
  }
}
