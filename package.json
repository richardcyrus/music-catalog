{
  "name": "your-score",
  "version": "1.0.1",
  "repository": "https://github.com/richardcyrus/music-catalog.git",
  "author": "Richard Cyrus <hello@rcyrus.com> (https://www.richardcyrus.com/)",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "axios": "0.19.2",
    "bcrypt": "5.0.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "helmet": "3.23.3",
    "if-env": "1.0.4",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.10.0",
    "mysql2": "2.1.0",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "sequelize": "5.14.0"
  },
  "devDependencies": {
    "concurrently": "5.2.0",
    "cross-env": "7.0.2",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.8",
    "husky": "4.2.5",
    "json": "9.0.6",
    "sequelize-cli": "5.5.0"
    "lint-staged": "10.2.11",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run start:prod || yarn run start:dev",
    "start:prod": "node ./bin/www",
    "start:dev": "concurrently \"cross-env DEBUG=your-score:* nodemon ./bin/www\" \"yarn run client\"",
    "client": "cd client && yarn start",
    "install": "cd client && yarn install",
    "build": "cd client && yarn build",
    "heroku-postbuild": "cd client && yarn --production=false && yarn build"
  },
  "lint-staged": {
    "**/*.{js,jsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": "^12.18.2",
    "yarn": "^1.22.4",
    "npm": "^6.14.5"
  }
}
